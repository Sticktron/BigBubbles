		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			
			<key>label</key>
			<string>Links</string>
		</dict>
		
		<!-- /r/jailbreak -->
		<dict>
			<key>action</key>
			<string>openReddit</string>
