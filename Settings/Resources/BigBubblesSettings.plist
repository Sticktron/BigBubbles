<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>title</key>
	<string>BigBubbles</string>
	
	<key>items</key>
	<array>
		
		
<!-- Stripe Cell Group -------------------------------------------------------->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>headerCellClass</key>
			<string>MGBBStripeCell</string>
		</dict>
		
		
		
<!-- Enabled Group ------------------------------------------------------------>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			
			<key>label</key>
			<string></string>
			
			<key>footerText</key>
			<string></string>
		</dict>
		
		<!-- Enabled Switch -->
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			
			<key>label</key>
			<string>Enabled</string>
			
			<key>id</key>
			<string>Enabled</string>
			<key>set</key>
			<string>setEnabledSwitch:specifier:</string>
			
			<key>key</key>
			<string>Enabled</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.sticktron.bigbubbles</string>
			
		</dict>
		
		
		
<!-- Appearance Group --------------------------------------------------------->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			
			<key>label</key>
			<string>Choose a size</string>
			
			<key>footerCellClass</key>
			<string>MGBBPreviewCell</string>
			
			<key>id</key>
			<string>SizeGroupCell</string>
		</dict>

	<!-- Size List -->
	<dict>
		<key>cell</key>
		<string>PSLinkListCell</string>
		
		<key>label</key>
		<string>Size</string>
		
		<key>detail</key>
		<string>PSListItemsController</string>
		
		<key>id</key>
		<string>BubbleSizeList</string>
		<key>set</key>
		<string>setSizeListValue:specifier:</string>
		
		<key>validTitles</key>
		<array>
			<string>Big (iOS 8 size)</string>
			<string>Bigger</string>
			<string>Custom</string>
		</array>
		
		<key>validValues</key>
		<array>
			<string>big</string>
			<string>bigger</string>
			<string>custom</string>
		</array>
		
		<key>key</key>
		<string>BubbleSizeName</string>
		<key>default</key>
		<string>custom</string>
		<key>defaults</key>
		<string>com.sticktron.bigbubbles</string>
	</dict>

	<!-- Custom Size Slider -->
	<dict>
		<key>cell</key>
		<string>PSSliderCell</string>
		
		<key>cellClass</key>
		<string>MGBBSliderCell</string>
		
		<key>label</key>
		<string>Bigger</string>
		
		<key>id</key>
		<string>PreviewSlider</string>
		
		<key>min</key>
		<real>0</real>
		<key>max</key>
		<real>1</real>
		
		<key>showValue</key>
		<true/>
		
		<key>leftImage</key>
		<string>icons/icon-minus.png</string>
		<key>rightImage</key>
		<string>icons/icon-plus.png</string>
		
		<key>key</key>
		<string>PreviewSliderValue</string>
		<key>default</key>
		<real>232</real>
		<key>defaults</key>
		<string>com.sticktron.bigbubbles</string>
	</dict>

	<!-- Apply Changes Button -->
	<dict>
		<key>cell</key>
		<string>PSButtonCell</string>
		
		<key>label</key>
		<string>Apply Changes</string>
		
		<key>action</key>
		<string>applyChanges</string>
	</dict>



<!-- Support Group ------------------------------------------------------------>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			
			<key>label</key>
			<string>Support</string>
		</dict>
		
		<!-- Email -->
		<dict>
			<key>cell</key>
			<string>PSButtonCell</string>
			
			<key>label</key>
			<string>sticktron@hotmail.com</string>
			
			<key>action</key>
			<string>openEmail</string>
			
			<key>icon</key>
			<string>icons/email.png</string>
		</dict>
		
		<!-- Tweet -->
		<dict>
			<key>action</key>
			<string>openTwitter</string>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>label</key>
			<string>@sticktron</string>
			<key>icon</key>
			<string>icons/twitter.png</string>
		</dict>
		
		
		
