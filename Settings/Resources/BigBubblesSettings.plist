			<string>openGitHub</string>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>label</key>
			<string>Source Code</string>
			<key>icon</key>
			<string>icons/github.png</string>
		</dict>
		
		<!-- Donate -->
		<dict>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>label</key>
			<string>Donate üç™</string>
			<key>icon</key>
			<string>icons/paypal.png</string>
			<key>action</key>
			<string>openPayPal</string>
		</dict>



<!-- Credits ------------------------------------------------------------------>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string></string>
			<key>footerText</key>
			<string>Sticktron VS Raymi 2014 üòà</string>
		</dict>
		
		
	</array>
</dict>
